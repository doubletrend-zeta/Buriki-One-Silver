;------------------------------------------------------------------------------
;Judge's Decision
;This section concerns judges' decision.
;Judges' Decision is a mechanic that compares the score earned by the two players over the round in the 
;case of a Time Out and overrides the normal time out system.
;------------------------------------------------------------------------------
;var(40) dictates the score for player 1
[State -2 set up Judges' Decision var]
type = varset
trigger1 = Roundstate < 2 && IsHomeTeam
;trigger1 = Roundstate = 2 && Teamside = 1 ;This section is the alternate version for if you want to play
v = 40			                   ;Arcade mode, as IsHomeTeam is the computer in Arcade Mode so
value = 0				   ;the vars would not be active. Be sure to change the P2 also.

[State -2 calculate score for standard attacks]
type = varadd
trigger1 = (stateno = [200,499]) && movehit = 1
v = 40
value = ceil(100*1.08)
;value = floor(target,gethitvar(damage)*1.08)

[State -2 calculate score for grabs]
type = varadd
trigger1 = P2stateno = 820 && time = 0
v = 40
value = 300

[State -2 calculate score for Special attacks]
type = varadd
trigger1 = numtarget
trigger1 = (stateno = [1000,1999]) && movehit = 1
v = 40
value = ceil(100*1.53)

[State -2 calculate score for Super attacks]
type = varadd
trigger1 = (stateno = [3000,3999]) && movehit = 1
v = 40
value = ceil(100*1.75)

[State -2 calculate score for Grapples]
type = varadd
trigger1 = numtarget
trigger1 = (stateno = [6000,6999]) && movehit = 1
v = 40
value = floor(hitcount*400)
;------------------------------------------------------------------------------
;var(41) dictates the score for player 2
[State -2 set up Judges' Decision var]
type = varset
trigger1 = (Roundstate < 2) && IsHomeTeam
;trigger1 = Roundstate = 2 && Teamside = 1
v = 41
value = 0

[State -2 calculate score for standard attacks]
type = varadd
trigger1 = (P2stateno = [200,499]) && time = 0
v = 41
value = ceil(100*1.08)

[State -2 calculate score for grabs]
type = varadd
trigger1 = (stateno = 820) && time = 0
v = 41
value = 300

[State -2 calculate score for Special attacks]
type = varadd
trigger1 = (P2stateno = [1000,1999]) && time = 0
v = 41
value = ceil(100*1.53)

[State -2 calculate score for Super attacks]
type = varadd
trigger1 = (P2stateno = [3000,3999]) && time = 0
v = 41
value = ceil(100*1.75)

[State -2 calculate score for Grapples]
type = varadd
trigger1 = (stateno = [6000,6999]) && time = 0
v = 41
value = floor(gethitvar(hitcount)*400)
;------------------------------------------------------------------------------
;These states act alongside Judges' Decision
[State -2 time out override]
type = assertspecial
trigger1 = !WinKO
trigger1 = time = [1,300]
flag = roundnotover

[State -2 compare score & P1 win]
type = LifeAdd
trigger1 = RoundState = 3
trigger1 = !WinKO
trigger1 = var(40) > var(41)
value = 1000

[State -2 compare score & P2 win]
type = LifeAdd
trigger1 = RoundState = 3
trigger1 = !WinKO
trigger1 = var(40) < var(41)
value = -1000

